    "def fizzbuzz(n):\n",
    "    for i in range(1, n + 1):\n",
    "        str = \"\"\n",
    "        if (i % 3) == 0:\n",
    "            str += \"fizz\"\n",
    "        if (i % 5) == 0:\n",
    "            str += \"buzz\"\n",
    "        print(i, str)"
   ]
